{
  "name": "strava-enhancer",
  "description": "A plugin to add bulk editing to activities to Strava.com.",
  "version": "0.1.2",
  "author": "Luke Karrys <luke@lukekarrys.com>",
  "devDependencies": {
    "@lukekarrys/eslint-config": "^1.0.30",
    "babel": "^5.8.34",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^5.4.0",
    "eslint": "^1.10.1",
    "eslint-plugin-classes": "^0.1.1",
    "eslint-plugin-react": "^3.10.0",
    "precommit-hook": "^3.0.0",
    "rimraf": "^2.4.4",
    "webpack": "^1.12.9"
  },
  "keywords": [
    "strava"
  ],
  "license": "MIT",
  "main": "dist/StravaEnhancer.js",
  "pre-commit": [
    "lint",
    "validate"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack -p src/index.js dist/StravaEnhancer.js",
    "lint": "eslint --ignore-path .gitignore .",
    "validate": "npm ls >/dev/null",
    "preversion": "npm run build && npm run validate && npm run lint",
    "publish-patch": "npm version patch && npm publish",
    "publish-minor": "npm version minor && npm publish",
    "publish-major": "npm version major && npm publish"
  }
}
